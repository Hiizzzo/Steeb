{
  "expo": {
    "name": "STEEB",
    "slug": "steeb",
    "scheme": "steeb",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icon.png",
    "ios": {
      "bundleIdentifier": "com.steeb.app",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.893679097790-mog5bng2hpkaipgk679ooh1cdh3pdpr2"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0931c3e7-2e46-4a00-a184-aeda30d06ae0"
      }
    }
  }
}